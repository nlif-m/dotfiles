#!/bin/sh

editor=emacs
configs="$HOME/.local/bin/openconfigs\n$HOME/.config/i3/config\n$HOME/.config/i3status/config.toml\n$HOME/.config/i3/i3.ms\n$HOME/.vimrc\n$HOME/.emacs.d/init.el\n$HOME/.config/zsh/.zshrc\n$HOME/.config/nvim/init.vim"
selected=$(printf $configs | rofi -dmenu) || exit
$editor $selected
